{
   "version": 2,
   "builds": [
     { "src": "*.js", "use": "@vercel/node" }
   ],
   "routes": [
     { "handle": "filesystem" },
     {
       "src": "/api/.*",
       "headers": {
         "Access-Control-Allow-Origin": "*"
       },
       "dest": "/api/*.js"
     },
     {
       "src": "/(.*)",
       "dest": "/$1.js"
     }
   ]
 }
 